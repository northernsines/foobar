{
  "Main Function": {
    "prefix": "main",
    "body": [
      "Main() {",
      "\t$1",
      "\treturn true;",
      "}"
    ],
    "description": "Create a Main function"
  },
  "Class": {
    "prefix": "class",
    "body": [
      "class ${1:CLASS_NAME} {",
      "\tprivate ${2:integer} ${3:fieldName};",
      "\t",
      "\tpublic Initialize(${4:integer} ${5:paramName}) {",
      "\t\tthisclass.${3:fieldName} = ${5:paramName};",
      "\t}",
      "\t",
      "\tpublic ${6:void} ${7:MethodName}() {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Create a class with Initialize"
  },
  "Class with Inheritance": {
    "prefix": "classi",
    "body": [
      "class ${1:CLASS_NAME} inherits ${2:PARENT_CLASS} {",
      "\tprivate ${3:integer} ${4:fieldName};",
      "\t",
      "\tpublic Initialize(${5:integer} ${6:paramName}) {",
      "\t\tthisclass.${4:fieldName} = ${6:paramName};",
      "\t}",
      "\t",
      "\tpublic ${7:void} ${8:MethodName}() {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "Create a class with inheritance"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if(${1:condition}) {",
      "\t$0",
      "}"
    ],
    "description": "If statement"
  },
  "If-Else Statement": {
    "prefix": "ifelse",
    "body": [
      "if(${1:condition}) {",
      "\t$2",
      "}",
      "else() {",
      "\t$0",
      "}"
    ],
    "description": "If-else statement"
  },
  "Loop For": {
    "prefix": "loopfor",
    "body": [
      "loop for(${1:10}) {",
      "\t$0",
      "}"
    ],
    "description": "Loop for N times"
  },
  "Loop Until": {
    "prefix": "loopuntil",
    "body": [
      "loop until(${1:condition}) {",
      "\t$0",
      "}"
    ],
    "description": "Loop until condition"
  },
  "Method": {
    "prefix": "method",
    "body": [
      "public ${1:void} ${2:MethodName}(${3:integer} ${4:paramName}) {",
      "\t$0",
      "}"
    ],
    "description": "Create a public method"
  },
  "Console Print": {
    "prefix": "print",
    "body": [
      "CONSOLE.Print(\"${1:message}\");"
    ],
    "description": "Print a message to console"
  },
  "Console PrintInteger": {
    "prefix": "printi",
    "body": [
      "CONSOLE.PrintInteger(${1:value});"
    ],
    "description": "Print an integer to console"
  },
  "Console PrintFloat": {
    "prefix": "printf",
    "body": [
      "CONSOLE.PrintFloat(${1:value});"
    ],
    "description": "Print a float to console"
  },
  "Console PrintBoolean": {
    "prefix": "printb",
    "body": [
      "CONSOLE.PrintBoolean(${1:value});"
    ],
    "description": "Print a boolean to console"
  },
  "Array Declaration": {
    "prefix": "array",
    "body": [
      "${1:integer}[] ${2:arrayName} = [${3:1, 2, 3, 4, 5}];"
    ],
    "description": "Declare and initialize an array"
  },
  "Lambda Expression": {
    "prefix": "lambda",
    "body": [
      "${1:x} -> ${2:x * 2}"
    ],
    "description": "Lambda expression"
  },
  "Map": {
    "prefix": "map",
    "body": [
      "${1:arrayName}.map(${2:x} -> ${3:x * 2})"
    ],
    "description": "Map over an array"
  },
  "Filter": {
    "prefix": "filter",
    "body": [
      "${1:arrayName}.filter(${2:x} -> ${3:x > 0})"
    ],
    "description": "Filter an array"
  },
  "Reduce": {
    "prefix": "reduce",
    "body": [
      "${1:arrayName}.reduce((${2:acc}, ${3:x}) -> ${4:acc + x}, ${5:0})"
    ],
    "description": "Reduce an array to a single value"
  },
  "MATH.Min": {
    "prefix": "mathmin",
    "body": ["MATH.Min(${1:a}, ${2:b})"],
    "description": "Get minimum of two numbers"
  },
  "MATH.Max": {
    "prefix": "mathmax",
    "body": ["MATH.Max(${1:a}, ${2:b})"],
    "description": "Get maximum of two numbers"
  },
  "MATH.Absolute": {
    "prefix": "mathabs",
    "body": ["MATH.Absolute(${1:value})"],
    "description": "Get absolute value"
  },
  "MATH.SquareRoot": {
    "prefix": "mathsqrt",
    "body": ["MATH.SquareRoot(${1:value})"],
    "description": "Get square root"
  },
  "MATH.Power": {
    "prefix": "mathpow",
    "body": ["MATH.Power(${1:base}, ${2:exponent})"],
    "description": "Raise to power"
  },
  "MATH.Random": {
    "prefix": "mathrand",
    "body": ["MATH.Random()"],
    "description": "Get random float between 0 and 1"
  },
  "STRING.Contains": {
    "prefix": "strcontains",
    "body": ["STRING.Contains(${1:text}, ${2:search})"],
    "description": "Check if string contains substring"
  },
  "STRING.StartsWith": {
    "prefix": "strstarts",
