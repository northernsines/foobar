  },
  "DATETIME.Now": {
    "prefix": "datenow",
    "body": ["integer ${1:timestamp} = DATETIME.Now();"],
    "description": "Get current timestamp"
  },
  "DATETIME.Year": {
    "prefix": "dateyear",
    "body": ["DATETIME.Year(${1:timestamp})"],
    "description": "Get year from timestamp"
  },
  "RANDOM.Integer": {
    "prefix": "randint",
    "body": ["RANDOM.Integer(${1:min}, ${2:max})"],
    "description": "Random integer in range"
  },
  "RANDOM.Float": {
    "prefix": "randfloat",
    "body": ["RANDOM.Float(${1:min}, ${2:max})"],
    "description": "Random float in range"
  },
  "RANDOM.Character": {
    "prefix": "randchar",
    "body": ["RANDOM.Character()"],
    "description": "Random printable character"
  },
  "RANDOM.Boolean": {
    "prefix": "randbool",
    "body": ["RANDOM.Boolean()"],
    "description": "Random boolean"
  },
  "FILE.Read": {
    "prefix": "fileread",
    "body": ["string ${1:content} = FILE.Read(\"${2:path.txt}\");"],
    "description": "Read file contents"
  },
  "FILE.Write": {
    "prefix": "filewrite",
    "body": ["FILE.Write(\"${1:path.txt}\", ${2:content});"],
    "description": "Write to file"
  },
  "FILE.Exists": {
    "prefix": "fileexists",
    "body": ["FILE.Exists(\"${1:path.txt}\")"],
    "description": "Check if file exists"
  },
  "Variable Declaration": {
    "prefix": "var",
    "body": [
      "${1:integer} ${2:variableName} = ${3:value};"
    ],
    "description": "Declare a variable with type and value"
  },
  "Class Field": {
    "prefix": "field",
    "body": [
      "private ${1:integer} ${2:fieldName};"
    ],
    "description": "Declare a private field in a class"
  },
  "For Each Element": {
    "prefix": "foreach",
    "body": [
      "// Process each element in ${1:arrayName}",
      "loop for(${1:arrayName}.length) {",
      "\t${2:integer} ${3:element} = ${1:arrayName}->data[i];",
      "\t$0",
      "}"
    ],
    "description": "Loop through each element in an array"
  }
}
