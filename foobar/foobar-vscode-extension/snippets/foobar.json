  },
  "Float toString": {
    "prefix": "floattostr",
    "body": ["${1:numVariable}.toString()"],
    "description": "Convert float to string"
  },
  "CONSOLE.ScanInteger": {
    "prefix": "scanint",
    "body": ["integer ${1:inputNum} = CONSOLE.ScanInteger();"],
    "description": "Read integer from input"
  },
  "CONSOLE.ScanFloat": {
    "prefix": "scanfloat",
    "body": ["float ${1:inputFloat} = CONSOLE.ScanFloat();"],
    "description": "Read float from input"
  },
  "CONSOLE.ScanBoolean": {
    "prefix": "scanbool",
