// Test inheritance

class SHAPE {
    private integer id;
    
    public Initialize(integer shapeId) {
        thisclass.id = shapeId;
    }
    
    public integer GetId() {
        return thisclass.id;
    }
}

class CIRCLE inherits SHAPE {
    private integer radius;
    
    public Initialize(integer shapeId, integer r) {
        parent.Initialize(shapeId);
        thisclass.radius = r;
    }
    
    public integer GetRadius() {
        return thisclass.radius;
    }
}

Main() {
    CONSOLE.Print("Testing inheritance...");
    
    CIRCLE c = new CIRCLE(1, 5);
    
    integer id = c.GetId();
    integer r = c.GetRadius();
    
    CONSOLE.Print("Circle ID:");
    CONSOLE.PrintInteger(id);
    CONSOLE.Print("Radius:");
    CONSOLE.PrintInteger(r);
    
    return true;
}
