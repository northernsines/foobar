Main() {
    integer x = 42;
    integer y = 10;
    
    CONSOLE.Print("Hello from FOOBAR!");
    CONSOLE.Print("Testing some math:");
    
    integer sum = x + y;
    CONSOLE.PrintInteger(sum);
    
    if(x > y) {
        CONSOLE.Print("x is greater than y");
    } else() {
        CONSOLE.Print("y is greater than or equal to x");
    }
    
    CONSOLE.Print("Counting to 5:");
    loop for(5) {
        CONSOLE.Print("Loop iteration");
    }
    
    return true;
}
