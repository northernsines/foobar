// FOOBAR Object-Oriented Programming Test

class CALCULATOR {
    private integer value;
    
    public Initialize(integer initialValue) {
        thisclass.value = initialValue;
    }
    
    public integer GetValue() {
        return thisclass.value;
    }
    
    public integer Add(integer amount) {
        thisclass.value = thisclass.value + amount;
        return thisclass.value;
    }
    
    public void SetValue(integer newValue) {
        thisclass.value = newValue;
    }
}

class SCIENTIFIC_CALCULATOR inherits CALCULATOR {
    private integer precision;
    
    public Initialize(integer initialValue, integer prec) {
        parent.SetValue(initialValue);
        thisclass.precision = prec;
    }
    
    public integer GetPrecision() {
        return thisclass.precision;
    }
    
    public integer Square() {
        integer val = thisclass.GetValue();
        return val * val;
    }
}

Main() {
    CONSOLE.Print("=== FOOBAR OOP Test ===");
    CONSOLE.Print("");
    
    // Test 1: Basic class instantiation
    CONSOLE.Print("Test 1: Creating a CALCULATOR");
    CALCULATOR calc = new CALCULATOR(10);
    integer value = calc.GetValue();
    CONSOLE.Print("Initial value: ");
    CONSOLE.PrintInteger(value);
    CONSOLE.Print("");
    
    // Test 2: Method calls
    CONSOLE.Print("Test 2: Adding 5");
    integer result = calc.Add(5);
    CONSOLE.Print("New value: ");
    CONSOLE.PrintInteger(result);
    CONSOLE.Print("");
    
    // Test 3: Inheritance
    CONSOLE.Print("Test 3: Creating SCIENTIFIC_CALCULATOR");
    SCIENTIFIC_CALCULATOR sciCalc = new SCIENTIFIC_CALCULATOR(7, 2);
    integer sciValue = sciCalc.GetValue();
    CONSOLE.Print("Scientific calc value: ");
    CONSOLE.PrintInteger(sciValue);
    integer prec = sciCalc.GetPrecision();
    CONSOLE.Print("Precision: ");
    CONSOLE.PrintInteger(prec);
    CONSOLE.Print("");
    
    // Test 4: Inherited methods
    CONSOLE.Print("Test 4: Using inherited methods");
    integer squared = sciCalc.Square();
    CONSOLE.Print("7 squared = ");
    CONSOLE.PrintInteger(squared);
    CONSOLE.Print("");
    
    // Test 5: Type checking with isa
    CONSOLE.Print("Test 5: Type checking");
    if(calc isa CALCULATOR) {
        CONSOLE.Print("calc is a CALCULATOR - correct!");
    }
    
    if(sciCalc isa SCIENTIFIC_CALCULATOR) {
        CONSOLE.Print("sciCalc is a SCIENTIFIC_CALCULATOR - correct!");
    }
    CONSOLE.Print("");
    
    CONSOLE.Print("=== All OOP tests passed! ===");
    
    return true;
}
